Package: vicinity
Title: Model Spatial Attributes with Ready4
Version: 0.0.0.9219
Authors@R: c(
    person("Matthew", "Hamilton", , "matthew.hamilton@orygen.org.au", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0001-7407-9194")),
